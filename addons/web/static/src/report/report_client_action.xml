<?xml version="1.0" encoding="UTF-8"?>
<templates>
    <t t-name="web.ReportClientAction" owl="1">
        <div class="h-100">
            <ControlPanel t-on-breadcrumb-clicked="breadcrumbClicked"
                title="title"
                action="props.action"
                breadcrumbs="breadcrumbs"
                withSearchBar="false"
            >
                <t t-set-slot="buttons">
                    <button t-on-click="print" type="button" class="btn btn-primary o_report_print" title="Print">Print</button>
                </t>
            </ControlPanel>
            <iframe t-ref="iframe" t-on-load="onIframeLoaded" class="o_report_iframe" t-att-src="report_url" />
        </div>
    </t>
</templates>
